<%= render "layouts/errors", :object => @update %>

<div class="container text-center">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
    <%= form_for @update do |f| %>
      <%= f.text_field :lastname, class: 'form-control', placeholder: 'Last Name' %>
      <br>
      <%= f.text_field :firstname, class: 'form-control', placeholder: 'First Name' %>
      <br>
      <%= f.text_field :age, class: 'form-control', placeholder: 'Age' %>
      <br>
      <%= f.text_field :gender, class: 'form-control', placeholder: 'Gender' %>
      <br>
      <%= f.text_field :cohort, class: 'form-control', placeholder: 'Cohort' %>
      <br>
      <%= f.text_field :internship, class: 'form-control', placeholder: 'Internship' %>
      <br>
      <%= f.text_field :company, class: 'form-control', placeholder: 'Company' %>
      <br>
      <%= f.submit class: 'btn btn-default'%>
    <% end %>
    </div>
  </div>
</div>
